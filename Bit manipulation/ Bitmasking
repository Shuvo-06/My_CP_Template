#include <bits/stdc++.h>
using namespace std;

int main(){
    //Bitmasking technique
    //Generating subsets problem
    //time complexity -> o(2^n)
    //generally n <= 20 for bitmasking problems

    int n;
    cin >> n;
    vector <int> v(n);
    for (int i=0; i<n; i++) cin >> v[i] ;
    
    for (long long int mask=0; mask < (1LL<<n); mask++){
        for (int j=0; j<n; j++){
            if ( (mask>>j) & 1 ) cout << v[j] << " ";
        }
        cout << "\n";
    }
    return 0;
}